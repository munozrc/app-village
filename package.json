{
  "name": "softlink",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "ts-standard",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "generate-programs-data": "npx ts-node --skip-project src/scripts/generate-programs-data.ts"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "7.1.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-router-dom": "5.1.7",
    "@vitejs/plugin-react": "^1.0.7",
    "gh-pages": "3.2.3",
    "ts-standard": "10.0.0",
    "typescript": "4.4.4",
    "vite": "2.6.13"
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "./node_modules/ts-standard/eslintrc.json"
    ]
  }
}
